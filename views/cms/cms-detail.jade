extends ../layout

block content

    div(class="content")
        div(class="container-fluid")
            div(class="row")
                div(class="col-md-12")
                    div(class="card")
                        div(class="card-header card-header-primary")
                            h4(class="card-title") CMS Details
                            p(class="card-category") Users details

                        div(class="card-body")
                            include ../alert
                            div(class="row")
                                div(class="col-lg-6 col-md-6 col-sm-6")
                                    div(class="card detail-card" )
                                        div(class="card-header ")
                                            h4(class="card-title") CMS Title : 
                                                h3(class="card-text")= cms.title
                            
                                div(class="col-lg-6 col-md-6 col-sm-6")
                                        div(class="card detail-card" )
                                            div(class="card-header ")
                                                h4(class="card-title") CMS Sub Title : 
                                                    h3(class="card-text")= cms.subtitle
                            
                                div(class="col-lg-12 col-md-6 col-sm-6")
                                        div(class="card detail-card" )
                                            div(class="card-header ")
                                                h4(class="card-title") Description : 
                                                    h3(class="card-text" )!= cms.description

                                